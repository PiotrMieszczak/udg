{
  "name": "csv-editor",
  "version": "0.0.0",
  "authors": [
    "Piotr Mieszczak <mieszczak.piotr@gmail.com>"
  ],
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:prod": "ng build --prod",
    "watch": "ng build --watch --configuration development",
    "test": "jest",
    "commit": "npx git-cz",
    "prepare": "husky install",
    "lint": "eslint --fix",
    "prettier:check": "prettier --config .prettierrc --write  --check",
    "postinstall": "ngcc --async"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "src/**/*.{ts,js}": "npm run lint",
    "src/**/*.{css,html}": "npm run prettier:check"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~13.0.0",
    "@angular/common": "~13.0.0",
    "@angular/compiler": "~13.0.0",
    "@angular/core": "~13.0.0",
    "@angular/forms": "~13.0.0",
    "@angular/platform-browser": "~13.0.0",
    "@angular/platform-browser-dynamic": "~13.0.0",
    "@angular/router": "~13.0.0",
    "@datorama/akita": "^6.1.3",
    "@datorama/akita-ng-entity-service": "^6.2.0",
    "@taiga-ui/cdk": "^2.43.0",
    "@taiga-ui/core": "^2.43.0",
    "@taiga-ui/icons": "^2.43.0",
    "@taiga-ui/kit": "^2.43.0",
    "ag-grid-angular": "^27.2.1",
    "ag-grid-community": "^27.2.1",
    "rxjs": "~7.4.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^13.3.5",
    "@angular-eslint/builder": "13.2.1",
    "@angular-eslint/eslint-plugin": "13.2.1",
    "@angular-eslint/eslint-plugin-template": "13.2.1",
    "@angular-eslint/schematics": "13.2.1",
    "@angular-eslint/template-parser": "13.2.1",
    "@angular/cli": "~13.0.4",
    "@angular/compiler-cli": "~13.0.0",
    "@ngneat/spectator": "^10.0.1",
    "@types/jest": "^27.5.1",
    "@types/node": "^12.11.1",
    "@typescript-eslint/eslint-plugin": "5.17.0",
    "@typescript-eslint/parser": "5.17.0",
    "eslint": "^8.12.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "git-cz": "^4.8.0",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "jest-preset-angular": "^11.1.2",
    "lint-staged": "^12.4.1",
    "prettier": "^2.6.2",
    "pretty-quick": "^3.1.3",
    "stylelint": "^14.8.2",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-standard": "^25.0.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "~4.4.3"
  }
}
